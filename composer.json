{
    "name": "mindforce/cakephp-garderobe-bootstrap-kit",
    "description": "Garderobe/BootstrapKit plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "~4.0",
        "cakephp/migrations": "*",
        "mindforce/cakephp-garderobe-bootstrap3": "dev-cakephp4"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Garderobe\\BootstrapKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Garderobe\\BootstrapKit\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/e2e4gu/cakephp-garderobe-bootstrap3.git"
        }
    ]
}
