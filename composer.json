{
    "name": "mindforce/cakephp-garderobe-bootstrap-kit",
    "description": "Garderobe/BootstrapKit plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "3.0.x-dev",
        "mindforce/garderobe-bootstrap3": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Garderobe\\BootstrapKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Garderobe\\BootstrapKit\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
